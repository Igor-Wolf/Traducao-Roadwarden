    # game/areas/eudociahouse2specifictopics.rpy:2171
    old "“They aren’t good for you, you know.”"
    new ""

    # game/areas/eudociahouse2specifictopics.rpy:2171
    old "“Here you go.”"
    new ""

    # game/areas/eudociahouse2specifictopics.rpy:2178
